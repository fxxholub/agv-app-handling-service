<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>6ec5602e-fdc0-4765-8863-8aacbe74e21d</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <DockerfileContext>.</DockerfileContext>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Scripts\djirpi\djirpi_battery.sh" />
    <None Remove="Scripts\djirpi\djirpi_bms.sh" />
    <None Remove="Scripts\djirpi\djirpi_bms_gpio.sh" />
    <None Remove="Scripts\djirpi\djirpi_platform.sh" />
    <None Remove="Scripts\djirpi\djirpi_scanner_1.sh" />
    <None Remove="Scripts\djirpi\djirpi_scanner_2.sh" />
    <None Remove="Scripts\djirpi\djirpi_scan_merge.sh" />
    <None Remove="Scripts\djirpi_platform.sh" />
    <None Remove="Scripts\djirpi_scanner_1.sh" />
    <None Remove="Scripts\djirpi_scanner_2.sh" />
    <None Remove="Scripts\djirpi_scan_merge.sh" />
    <None Remove="Scripts\leplinux\leplinux_navigation_bringup.sh" />
    <None Remove="Scripts\leplinux\leplinux_navigation_collision.sh" />
